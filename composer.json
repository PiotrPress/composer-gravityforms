{
    "name": "piotrpress/composer-gravityforms",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "This composer plugin enables installation of GravityForms WordPress plugin and its addons.",
    "homepage": "https://github.com/PiotrPress/composer-gravityforms",
    "authors": [
        {
            "name": "Piotr Niewiadomski",
            "homepage": "https://piotr.press"
        }
    ],
    "keywords": [
        "composer", "composer plugin", "gravityforms", "wordpress", "plugin", "addon", "addons", "package", "packages", "php"
    ],
    "require": {
        "php": ">=7.4",
        "composer-plugin-api": "^2.0",
        "piotrpress/streamer": "^1.0"
    },
    "extra": {
        "class": "PiotrPress\\Composer\\GravityForms\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "PiotrPress\\Composer\\GravityForms\\": "src/"
        },
        "classmap": [
            "src"
        ]
    }
}